{
  "name": "milind/laravel-pubsub",
  "description": "A Pub-Sub abstraction for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Milind",
      "email": "milind@getflits.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
    "milind/php-pubsub": "^1.0|^2.0|^3.0",
    "milind/php-pubsub-redis": "^1.0|^2.0|^3.0",
    "milind/php-pubsub-google-cloud": "^5.0|^6.0",
    "illuminate/console": "^5.4|^6.0|^7.0|^8.0",
    "milind/php-pubsub-http": "^1.0|^2.0"
  },
  "autoload": {
    "psr-4": {
      "Superbalist\\LaravelPubSub\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  }
}
